{
    "name": "cryptuoso",
    "version": "3.0.2",
    "license": "MIT",
    "repository": "https://github.com/cryptuoso/cryptuoso",
    "author": "Cryptuoso Team",
    "keywords": [
        "cryptocurrency",
        "trading",
        "robots",
        "cryptuoso"
    ],
    "scripts": {
        "nx": "nx",
        "start": "nx run-many --target=serve --all --parallel --maxParallel=20",
        "build": "nx run-many --target=build --all --parallel --progress --with-deps --memoryLimit=512",
        "test": "nx run-many --target=test --all --parallel --with-deps",
        "lint": "nx workspace-lint && nx run-many --target=lint --all --parallel",
        "e2e": "nx e2e",
        "affected:apps": "nx affected:apps --plain",
        "affected:libs": "nx affected:libs --plain",
        "affected:build": "nx affected:build",
        "affected:e2e": "nx affected:e2e",
        "affected:test": "nx affected:test",
        "affected:lint": "nx affected:lint",
        "affected:dep-graph": "nx affected:dep-graph",
        "affected": "nx affected",
        "format": "nx format:write",
        "format:write": "nx format:write",
        "format:check": "nx format:check",
        "update": "nx migrate latest",
        "workspace-schematic": "nx workspace-schematic",
        "dep-graph": "nx dep-graph",
        "help": "nx help",
        "gen:app": "nx generate @nrwl/node:application",
        "gen:lib": "nx generate @nrwl/node:library",
        "build:importer-runner": "nx build importer-runner --prod",
        "serve:importer-runner": "nx serve importer-runner",
        "build:importer-worker": "nx build importer-worker --prod",
        "serve:importer-worker": "nx serve importer-worker",
        "build:exwatcher-runner": "nx build exwatcher-runner --prod",
        "serve:exwatcher-runner": "nx serve exwatcher-runner",
        "build:exwatcher-binance": "nx build exwatcher-binance --prod",
        "serve:exwatcher-binance": "nx serve exwatcher-binance",
        "build:exwatcher-bitfinex": "nx build exwatcher-bitfinex --prod",
        "serve:exwatcher-bitfinex": "nx serve exwatcher-bitfinex",
        "build:exwatcher-kraken": "nx build exwatcher-kraken --prod",
        "serve:exwatcher-kraken": "nx serve exwatcher-kraken"
    },
    "private": true,
    "dependencies": {
        "@luminati-io/socksv5": "0.0.7",
        "async-retry": "^1.3.1",
        "body-parser": "^1.19.0",
        "bullmq": "^1.8.14",
        "ccxt": "^1.31.24",
        "ccxt.pro": "git+ssh://git@github.com/deman4ik/ccxt.pro.git",
        "cloudevents-sdk": "^2.0.2",
        "dayjs": "^1.8.29",
        "fastest-validator": "^1.5.1",
        "helmet": "^3.23.3",
        "https-proxy-agent": "^5.0.0",
        "ioredis": "^4.17.3",
        "lightship": "^6.1.0",
        "middleware-if-unless": "^1.2.4",
        "node-cron": "^2.0.3",
        "node-fetch": "^2.6.0",
        "pg-native": "^3.0.0",
        "postgres": "^2.0.0-beta.0",
        "restana": "^4.6.2",
        "scramjet": "^4.33.4",
        "slonik": "^22.4.11",
        "slonik-interceptor-field-name-transformation": "^1.5.3",
        "threads": "^1.6.3",
        "tslog": "^2.6.0",
        "uuid": "^8.2.0"
    },
    "devDependencies": {
        "@nrwl/eslint-plugin-nx": "9.4.2",
        "@nrwl/jest": "9.4.2",
        "@nrwl/node": "9.4.2",
        "@nrwl/workspace": "9.4.2",
        "@types/async-retry": "^1.4.2",
        "@types/body-parser": "^1.19.0",
        "@types/helmet": "0.0.47",
        "@types/jest": "25.1.4",
        "@types/lightship": "^6.1.0",
        "@types/node": "~8.9.4",
        "@types/node-cron": "^2.0.3",
        "@types/node-fetch": "^2.5.7",
        "@types/slonik": "^22.1.2",
        "@types/uuid": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "2.19.2",
        "@typescript-eslint/parser": "2.19.2",
        "dotenv": "6.2.0",
        "eslint": "6.8.0",
        "eslint-config-prettier": "6.0.0",
        "eslint-plugin-prettier": "^3.1.4",
        "jest": "25.2.3",
        "mockdate": "^3.0.2",
        "prettier": "2.0.4",
        "ts-jest": "25.2.1",
        "ts-node": "~7.0.0",
        "typescript": "^3.9.6"
    }
}
