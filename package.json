{
  "name": "cryptuoso",
  "version": "3.0.0",
  "license": "MIT",
  "repository": "https://github.com/cryptuoso/cryptuoso",
  "author": "Cryptuoso Team",
  "keywords": [
    "cryptocurrency",
    "trading",
    "robots"
  ],
  "scripts": {
    "nx": "nx",
    "start": "nx run-many --target=serve --all --parallel",
    "build": "nx run-many --target=build --all --parallel",
    "test": "nx run-many --target=test --all --parallel --with-deps",
    "lint": "nx workspace-lint && nx run-many --target=lint --all --parallel",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps --plain",
    "affected:libs": "nx affected:libs --plain",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "gen:app": "nx generate @nrwl/node:application",
    "gen:lib": "nx generate @nrwl/node:library",
    "build:importer-runner": "nx build importer-runner --prod",
    "serve:importer-runner": "nx serve importer-runner",
    "build:importer-worker": "nx build importer-worker --prod",
    "serve:importer-worker": "nx serve importer-worker",
    "serve:mail-publisher": "nx serve mail-publisher",
    "build:mail-publisher": "nx build mail-publisher --prod"
  },
  "private": true,
  "dependencies": {
    "@types/mailgun-js": "^0.22.7",
    "async-retry": "^1.3.1",
    "body-parser": "^1.19.0",
    "bullmq": "^1.8.13",
    "ccxt": "^1.30.42",
    "cloudevents-sdk": "^2.0.2",
    "dayjs": "^1.8.28",
    "fastest-validator": "^1.5.1",
    "helmet": "^3.23.2",
    "https-proxy-agent": "^5.0.0",
    "ioredis": "^4.17.3",
    "lightship": "^6.1.0",
    "mailgun-js": "^0.22.0",
    "middleware-if-unless": "^1.2.4",
    "node-fetch": "^2.6.0",
    "postgres": "^2.0.0-beta.0",
    "restana": "^4.6.1",
    "scramjet": "^4.33.0",
    "threads": "^1.6.2",
    "tslog": "^2.5.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@nrwl/eslint-plugin-nx": "9.4.2",
    "@nrwl/jest": "9.4.2",
    "@nrwl/node": "9.4.2",
    "@nrwl/workspace": "9.4.2",
    "@types/async-retry": "^1.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "25.1.4",
    "@types/lightship": "^6.1.0",
    "@types/node": "~8.9.4",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "25.2.3",
    "mockdate": "^3.0.2",
    "prettier": "2.0.4",
    "ts-jest": "25.2.1",
    "ts-node": "~7.0.0",
    "typescript": "^3.9.5"
  }
}
